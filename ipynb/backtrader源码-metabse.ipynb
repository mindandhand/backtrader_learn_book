   "source": [
    "def findbases(kls, topclass):\n",
    "    retval = list()\n",
    "    for base in kls.__bases__:\n",
    "        if issubclass(base, topclass):\n",
    "            retval.extend(findbases(base, topclass))\n",
    "            retval.append(base)\n",
    "\n",
    "    return retval"
   ]
